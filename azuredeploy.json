{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string"
			"defaultValue": "West2us"		
			"allowedValues": [
				"West2us",
				"Westus"
		]
        },
        "virtualNetworkName": {
            "type": "string",
			"defaultValue": "VNET101_HUB",
			"allowedValues": [
				"VNET101_HUB",
				"VNET102_Prod",
				"VNET103_VDI"
			]
        },
        "resourceGroup": {
            "type": "string",
			"defaultValue": "RG_HUB"
        },
        "addressSpaces": {
            "type": "array"
			"defaultValue": "10.222.0.0/16"
        },
        "ipv6Enabled": {
            "type": "bool"
        },
        "subnetCount": {
            "type": "int"
        },
        "subnet0_name": {
            "type": "string"
			"defaultValue": "FrontEnd"
        },
        "subnet0_addressRange": {
            "type": "string"
        },
    },
    "variables": {},
    "resources": [
        {
            "name": "[parameters('virtualNetworkName')]",
            "type": "Microsoft.Network/VirtualNetworks",
            "apiVersion": "2019-09-01",
            "location": "[parameters('location')]",
            "dependsOn": [],
            "tags": {},
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.1.0.0/16",
                        "10.10.0.0/16"
                    ]
                },
                "subnets": [
                    {
                        "name": "FrontEnd",
                        "properties": {
                            "addressPrefix": "10.10.1.0/24"
                        }
                    }
                ],
                "enableDdosProtection": "[parameters('ddosProtectionPlanEnabled')]"
            }
        }
    ]
}
